{
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": 0,
        "arrow-parens": "off",
        "object-curly-newline": 0,
        "import/prefer-default-export": 0,
        "camelcase": "off",
        "react/prop-types": "off",
        "react/jsx-props-no-spreading": 0,
        "react/no-danger": 0,
        "no-use-before-define": "off"
    }
}
